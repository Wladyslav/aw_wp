{{ function('get_header', '') }}

<ol class="nav navbar-nav breadcrumbs" itemscope itemtype=http://schema.org/BreadcrumbList>
        <li itemprop=itemListElement itemscope itemtype=http://schema.org/ListItem><a href=/ itemprop=item><span itemprop=name>Agencja Wrocławska</span>
                <meta itemprop=position content="1">
            </a>></li>
        <li class=active itemprop=itemListElement itemscope itemtype=http://schema.org/ListItem><span itemprop=name>Kariera</span>
            <meta itemprop=position content="2">
        </li>
    </ol>
    <main id=content>
        <section class="sec-padding kariera-page">
            <h1 class='blog-header txt-XXL f-w700 txt-underline txt-red'>Kariera w Agencji Wrocławskiej</h1>
            <div class=container>
                <div class=row>
                    <div class=before-offers>
                        <p>Obecnie rekrutujemy na poniższe stanowiska:</p>
                    </div>
                </div>
            </div>
        </section>
        <section class="sec-padding offers-listing-section">
            <div class=container>
                <div class=row>
                    <p class="header-template offers-listing-header"><span class="header-template f-w700 txt-black txt-XL ornament-r-b">Aktualnie poszukujemy</span></p>
                    <ul class="offers-list with_flex">
                        {% for k in kariera %}
                        <li><a href={{k.path}} >
                            <p class=h3>{{k.post_title}}</p>
                            <div class=art-summary>{{k.meta('miasto')}}</div></a>
                        </li>
                        {% endfor %}
                    </ul>
                </div>
            </div>
        </section>
        <section class='sec-padding sc-txt-in-middle'>
            <div class=middl-text-cnt>
                <p></p>
            </div>
        </section>
    </main><svg class="svg">
        <clipPath id="my-clip-path" clipPathUnits="objectBoundingBox">
            <path d="M1 0S.734.31.0.148v.733s.353.254 1 .02V0" />
        </clipPath>
    </svg><svg class="svg">
        <clipPath id="my-clip-path2" clipPathUnits="objectBoundingBox">
            <path d="M1 .171S.95.13.857.177C.711.25.447.413.0.316v.721s.353.249 1 .019V.171" />
        </clipPath>
    </svg>
